{
  "compilerOptions": {
    "target": "ESNext",  
    "lib": ["ESNext"],
    "outDir": "./dist",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "composite": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules","dist"]
}
